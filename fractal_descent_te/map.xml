<map proto="1.4.0">
<time overtime="1m" max-overtime="15m">45m</time>
<name>Fractal Descent TE</name>
<version>1.3.6</version>
<objective>Leak lava from the enemy team's core and destroy their end stone monument.</objective>
<authors>
    <author uuid="dbe909c6-2b15-4528-b12a-c77ed4031496" contribution="Design and building"/> <!-- teheggy -->
    <author uuid="459fd919-0d58-401a-9601-6cab1eb721ab" contribution="Design, building, and XML"/> <!-- Reshif -->
</authors>
<teams>
    <team id="blue" color="blue" min="6" max="8" max-overfill="8">Blue</team>
    <team id="red" color="dark red" min="6" max="8" max-overfill="8">Red</team>
</teams>
<kits>
    <kit id="spawn">
        <item slot="0" unbreakable="true">diamond sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow infinite:1">bow</item>
        <item slot="28" amount="1">arrow</item>
        <item slot="2" unbreakable="true">diamond pickaxe</item>
        <item slot="3" unbreakable="true" enchantment="dig speed:1">iron axe</item>
        <item slot="4">golden apple</item>
        <item slot="5" amount="64">smooth brick</item>
        <item slot="32" amount="64">smooth brick</item>
        <item slot="6" amount="64">glass</item>
        <item slot="7" amount="64" damage="1">wood</item>
        <item slot="34" amount="32">ladder</item>
        <item slot="8" amount="32">golden carrot</item>
        <effect duration="7" amplifier="1">heal</effect>
        <effect duration="7">damage resistance</effect>
    </kit>
    <kit id="kill-reward">
        <effect amplifier="2" duration="5">regeneration</effect>
    </kit>
</kits>
<filters>
    <deny id="only-blue">
        <team>red</team>
    </deny>
    <deny id="only-red">
        <team>blue</team>
    </deny>
    <deny id="no-repeaters">
        <any>
            <material>diode block off</material>
            <material>diode block on</material>
        </any>
    </deny>
    <deny id="no-stone">
        <material>stone</material>
    </deny>
    <deny id="no-dispensers">
        <material>dispenser</material>
    </deny>
    <material id="tnt-at-height-limit">tnt</material>
</filters>
<regions>
    <negative id="void-area">
        <rectangle id="playable-area" min="-42,-92" max="42,92"/>
    </negative>
    <union id="bases">
        <circle id="blue-base" center="0,-84" radius="10"/>
        <circle id="red-base" center="0,84" radius="10"/>
    </union>
    <union id="spawns">
        <cylinder id="blue-spawn" base="0,18.5,-132" radius="1" height="1"/>
        <cylinder id="red-spawn" base="0,18.5,132" radius="1" height="1"/>
    </union>
    <union id="height-limits">
        <above id="tnt-height-limit" y="60"/>
        <above id="above-height-limit" y="61"/>
    </union>
    <apply block="never" message="You may not interact with blocks outside the map!" region="void-area"/>
    <apply block="never" message="You may not destroy the spawn area!" region="bases"/>
    <apply block-place="tnt-at-height-limit" region="tnt-height-limit" message="Only TNT is allowed at this level!"/>
    <apply block="never" region="above-height-limit" message="You may not edit blocks at this level!"/>
    <apply block-place="no-repeaters" name="You may not use repeaters on this map!"/>
    <apply block-place="no-stone"/>
    <apply block="no-dispensers"/>
</regions>
<spawns>
    <default yaw="-90">
        <region>
            <cuboid min="-58,29,-2" max="-54,29,2"/>
        </region>
    </default>
    <spawn team="blue" kit="spawn" yaw="0">
        <region>
            <region id="blue-spawn"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn" yaw="-180">
        <region>
            <region id="red-spawn"/>
        </region>
    </spawn>
</spawns>
<destroyables materials="ender stone" completion="100%" proximity-metric="none">
    <destroyable name="East Monument" owner="blue">
        <region>
            <cuboid min="22,10,-31" max="26,16,-27"/>
        </region>
    </destroyable>
    <destroyable name="West Monument" owner="red">
        <region>
            <cuboid min="-26,10,27" max="-22,16,31"/>
        </region>
    </destroyable>
</destroyables>
<cores material="obsidian" leak="3" mode-changes="true" proximity-metric="none">
    <core team="blue">
        <region>
            <cuboid min="-27,11,-32" max="-21,17,26"/>
        </region>
    </core>
    <core team="red">
        <region>
            <cuboid min="-21,11,26" max="27,17,32"/>
        </region>
    </core>
</cores>
<modes>
    <mode after="10m" material="gold block" name="`6GOLD CORE MODE"/>
    <mode after="25m" material="packed ice" name="`3PACKED ICE CORE MODE"/>
</modes>
<toolrepair>
    <tool>diamond sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>obsidian</item>
    <item>gold block</item>
    <item>packed ice</item>
    <item>ender stone</item>
    <item>golden apple</item>
    <item>smooth brick</item>
    <item>glass</item>
    <item>wood</item>
    <item>golden carrot</item>
    <item>arrow</item>
    <item>ladder</item>
</itemremove>
<kill-reward>
    <kit id="kill-reward"/>
    <item amount="32">glass</item>
    <item amount="32" damage="1">wood</item>
</kill-reward>
<tnt>
    <dispenser-tnt-limit>0</dispenser-tnt-limit>
    <dispenser-tnt-multiplier>0</dispenser-tnt-multiplier>
</tnt>
<autorespawn/>
</map>
