<map proto="1.4.0">
<name>Siege</name>
<version>1.0.2</version>
<objective>Capture all five control points.</objective>
<authors>
    <author uuid="2958ea4c-3501-404e-88cc-12e8632f68cc"/> <!--  NixaniaXi  -->
</authors>
<teams>
    <team id="red" color="dark red" max="5" max-overfill="5">Red</team>
    <team id="blue" color="blue" max="5" max-overfill="5">Blue</team>
</teams>
<kits>
    <kit id="main-kit">
        <clear/>
        <game-mode>adventure</game-mode>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" amount="1" material="golden apple"/>
        <item slot="8" amount="32" material="arrow"/>
        <chestplate unbreakable="true" material="chainmail chestplate">
            <enchantment level="3">protection projectile</enchantment>
        </chestplate>
        <effect duration="4" amplifier="10">resistance</effect>
        <effect duration="4" amplifier="10">regeneration</effect>
    </kit>
    <kit id="blue-kit" parents="main-kit">
        <helmet unbreakable="true" color="0066cc" material="leather helmet"/>
        <leggings unbreakable="true" color="0066cc" material="leather leggings"/>
        <boots unbreakable="true" color="0066cc" material="leather boots"/>
    </kit>
    <kit id="red-kit" parents="main-kit">
        <helmet unbreakable="true" color="cd0000" material="leather helmet"/>
        <leggings unbreakable="true" color="cd0000" material="leather leggings"/>
        <boots unbreakable="true" color="cd0000" material="leather boots"/>
    </kit>
</kits>
<kill-reward>
    <item material="golden apple"/>
    <item amount="16" material="arrow"/>
</kill-reward>
<hunger>
    <depletion>off</depletion>
</hunger>
<spawns>
    <spawn team="red" filter="spawn-1" kit="red-kit">
        <regions yaw="180">
            <block>122.5,15,55.5</block>
        </regions>
    </spawn>
    <spawn team="red" filter="spawn-2" kit="red-kit">
        <regions yaw="180">
            <block>122.5,65,55.5</block>
        </regions>
    </spawn>
    <spawn team="red" filter="spawn-3" kit="red-kit">
        <regions yaw="180">
            <block>122.5,115,55.5</block>
        </regions>
    </spawn>
    <spawn team="red" filter="spawn-4" kit="red-kit">
        <regions yaw="180">
            <block>122.5,165,55.5</block>
        </regions>
    </spawn>
    <spawn team="red" filter="spawn-5" kit="red-kit">
        <regions yaw="180">
            <block>122.5,215,55.5</block>
        </regions>
    </spawn>
    <spawn team="red" filter="spawn-6" kit="red-kit">
        <regions yaw="180">
            <block>122.5,255,55.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-1" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,15,-142.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-2" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,65,-142.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-3" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,115,-142.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-4" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,165,-142.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-5" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,215,-142.5</block>
        </regions>
    </spawn>
    <spawn team="blue" filter="spawn-6" kit="blue-kit">
        <regions yaw="0">
            <block>122.5,255,-142.5</block>
        </regions>
    </spawn>
    <default>
        <regions angle="122.5,0,-43.5">
            <point>122.5,70,-43.5</point>
        </regions>
    </default>
</spawns>
<respawn delay="7s" auto="true"/>
<filters>
    <team id="only-blue">blue</team>
    <team id="only-red">red</team>
    <!-- Point Filters -->
    <!-- Blue Portal Filters -->
    <all id="blue-blue-portal">
        <objective>blue-2nd</objective>
        <not>
            <any>
                <objective>mid</objective>
                <objective>red-2nd</objective>
            </any>
        </not>
    </all>
    <all id="blue-mid-portal">
        <objective>blue-2nd</objective>
        <objective>mid</objective>
        <not>
            <objective>red-2nd</objective>
        </not>
    </all>
    <all id="blue-red-portal">
        <objective>blue-2nd</objective>
        <objective>mid</objective>
        <objective>red-2nd</objective>
    </all>
    <!-- Red Portal Filters -->
    <all id="red-red-portal">
        <objective>red-2nd</objective>
        <not>
            <any>
                <objective>mid</objective>
                <objective>blue-2nd</objective>
            </any>
        </not>
    </all>
    <all id="red-mid-portal">
        <objective>red-2nd</objective>
        <objective>mid</objective>
        <not>
            <objective>blue-2nd</objective>
        </not>
    </all>
    <all id="red-blue-portal">
        <objective>red-2nd</objective>
        <objective>mid</objective>
        <objective>blue-2nd</objective>
    </all>
    <!-- Time Filters -->
    <not id="spawn-1">
        <time>2m</time>
    </not>
    <all id="spawn-2">
        <time>2m</time>
        <not>
            <time>4m</time>
        </not>
    </all>
    <all id="spawn-3">
        <time>4m</time>
        <not>
            <time>6m</time>
        </not>
    </all>
    <all id="spawn-4">
        <time>6m</time>
        <not>
            <time>8m</time>
        </not>
    </all>
    <all id="spawn-5">
        <time>8m</time>
        <not>
            <time>10m</time>
        </not>
    </all>
    <all id="spawn-6">
        <time>10m</time>
    </all>
</filters>
<regions>
    <apply block="never"/>
</regions>
<control-points incremental="true" neutral-state="true" show-progress="true" scaled-time="true" time-multiplier="0.25" capture-time="5s" points="0" required="false">
    <control-point initial-owner="red" id="red-last" name="Red Last">
        <capture-region>
            <cuboid min="127,0,36" max="118,50,45"/>
        </capture-region>
        <progress-display-region>
            <cuboid min="127,0,36" max="118,50,45"/>
        </progress-display-region>
        <player-filter>
            <any>
                <objective>red-2nd</objective>
                <team>red</team>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="red" id="red-2nd" name="Red 2nd">
        <capture-region>
            <cuboid min="127,0,-3" max="118,50,7"/>
        </capture-region>
        <progress-display-region>
            <cuboid min="127,0,-3" max="118,50,7"/>
        </progress-display-region>
        <player-filter>
            <any>
                <objective>red-last</objective>
                <objective>mid</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point id="mid" name="Mid" points="1">
        <capture-region>
            <cuboid min="127,0,-48" max="118,50,-39"/>
        </capture-region>
        <progress-display-region>
            <cuboid min="127,0,-48" max="118,50,-39"/>
        </progress-display-region>
        <player-filter>
            <any>
                <objective>red-2nd</objective>
                <objective>blue-2nd</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="blue" id="blue-2nd" name="Blue 2nd">
        <capture-region>
            <cuboid min="127,0,-94" max="118,50,-84"/>
        </capture-region>
        <progress-display-region>
            <cuboid min="127,0,-94" max="118,50,-84"/>
        </progress-display-region>
        <player-filter>
            <any>
                <objective>mid</objective>
                <objective>blue-last</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="blue" id="blue-last" name="Blue Last">
        <capture-region>
            <cuboid min="127,0,-132" max="118,50,-123"/>
        </capture-region>
        <progress-display-region>
            <cuboid min="127,0,-132" max="118,50,-123"/>
        </progress-display-region>
        <player-filter>
            <any>
                <objective>blue-2nd</objective>
                <team>blue</team>
            </any>
        </player-filter>
    </control-point>
    <control-point id="end" points="10000" capture-time="0s" show="false">
        <capture-region><everywhere/></capture-region>
        <player-filter>
            <any>
                <all>
                    <objective>blue-last</objective>
                    <team>red</team>
                </all>
                <all>
                    <objective>red-last</objective>
                    <team>blue</team>
                </all>
            </any>
        </player-filter>
    </control-point>
</control-points>
<score>
    <kills>0</kills>
    <deaths>0</deaths>
    <limit>400</limit>
</score>
<portals>
    <!-- Red Portals -->
    <portal x="@142" y="@7" z="@17" yaw="@180" filter="red-red-portal">
        <region>
            <cuboid min="120,6,58" max="125,9,53"/>
        </region>
    </portal>
    <portal x="@137" y="@7" z="@-10" yaw="@90" filter="red-mid-portal">
        <region>
            <cuboid min="120,6,58" max="125,9,53"/>
        </region>
    </portal>
    <portal x="@135.5" y="@2" z="@-60.5" yaw="@90" filter="red-blue-portal">
        <region>
            <cuboid min="120,6,58" max="125,9,53"/>
        </region>
    </portal>
    <!-- Blue Portals -->
    <portal x="@103" y="@7" z="@-104" yaw="@0" filter="blue-blue-portal">
        <region>
            <cuboid min="120,6,-140" max="125,9,-145"/>
        </region>
    </portal>
    <portal x="@108" y="@7" z="@-77" yaw="@-90" filter="blue-mid-portal">
        <region>
            <cuboid min="120,6,-140" max="125,9,-145"/>
        </region>
    </portal>
    <portal x="@109.5" y="@2" z="@-26.5" yaw="@-90" filter="blue-red-portal">
        <region>
            <cuboid min="120,6,-140" max="125,9,-145"/>
        </region>
    </portal>
</portals>
<toolrepair>
	<tool>stone sword</tool>
	<tool>bow</tool>
</toolrepair>
<itemkeep>
    <item>arrow</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>leather helmet</item>
    <item>chainmail chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<timelock>true</timelock>
</map>
